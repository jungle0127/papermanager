  </resultMap>
  <resultMap extends="BaseResultMap" id="ResultMapWithBLOBs" type="com.ncee.dao.model.QuestionWithBLOBs">
    <result column="answer" jdbcType="LONGVARCHAR" property="answer" />
    <result column="analysis" jdbcType="LONGVARCHAR" property="analysis" />
  </resultMap>
  <sql id="Base_Column_List">
    question_id, question, question_type_id, construct_type_id, active
  </sql>
  <sql id="Blob_Column_List">
    answer, analysis
  </sql>
  <insert id="insertSelective" parameterType="com.ncee.dao.model.QuestionWithBLOBs">
    insert into question
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="questionId != null">
        question_id,
      </if>
      <if test="question != null">
        question,
      </if>
      <if test="questionTypeId != null">
        question_type_id,
      </if>
      <if test="constructTypeId != null">
        construct_type_id,
      </if>
      <if test="active != null">
        active,
      </if>
      <if test="answer != null">
        answer,
      </if>
      <if test="analysis != null">
        analysis,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="questionId != null">
        #{questionId,jdbcType=BIGINT},
      </if>
      <if test="question != null">
        #{question,jdbcType=VARCHAR},
      </if>
      <if test="questionTypeId != null">
        #{questionTypeId,jdbcType=BIGINT},
      </if>
      <if test="constructTypeId != null">
        #{constructTypeId,jdbcType=BIGINT},
      </if>
      <if test="active != null">
        #{active,jdbcType=INTEGER},
      </if>
      <if test="answer != null">
        #{answer,jdbcType=LONGVARCHAR},
      </if>
      <if test="analysis != null">
        #{analysis,jdbcType=LONGVARCHAR},
      </if>
    </trim>
  </insert>
  <update id="updateByPrimaryKeySelective" parameterType="com.ncee.dao.model.QuestionWithBLOBs">
    update question

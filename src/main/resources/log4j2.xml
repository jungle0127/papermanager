<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
	<properties>
		<property name="LOG_HOME">./ncee.log</property>
	</properties>
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<ThresholdFilter level="trace" onMatch="ACCEPT" onMismatch="DENY"/>
			<PatternLayout pattern="%d{yyyy-MM-dd HH:mmss.SSSS} %-5level %class{36} %L %M - %msg %xEx%n"/>
		</Console>
	</Appenders>
</Configuration>